#! /usr/bin/nickle

sum = 0;
sum_of_squares = 0;
count = 0;

while (!File::end(stdin)) {
	n = string_to_real(gets());
	sum += n;
	sum_of_squares += n ** 2;
	++count;
}

mean = sum / count;
variance = sum_of_squares / count - mean ** 2;
deviation = sqrt(variance);
error = sqrt(variance / count);

printf("%s,%g,%g\n", argv[1], mean, error);
